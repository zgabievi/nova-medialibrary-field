{
    "name": "dmitrybubyakin/nova-medialibrary-field",
    "description": "Laravel Nova field for managing the Spatie media library.",
    "keywords": [
        "laravel",
        "nova",
        "medialibrary"
    ],
    "homepage": "https://github.com/dmitrybubyakin/nova-medialibrary-field",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Bubyakin",
            "email": "dimabubyakin97@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "laravel/nova": "*",
        "spatie/laravel-medialibrary": "^10.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.6",
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "DmitryBubyakin\\NovaMedialibraryField\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "DmitryBubyakin\\NovaMedialibraryField\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DmitryBubyakin\\NovaMedialibraryField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "format": [
            "vendor/bin/php-cs-fixer fix --allow-risky=yes"
        ]
    }
}
